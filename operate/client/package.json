{
  "name": "camunda-operate",
  "version": "8.8.0-SNAPSHOT",
  "private": true,
  "type": "module",
  "dependencies": {
    "@axe-core/playwright": "4.10.0",
    "@bpmn-io/element-template-icon-renderer": "1.0.0",
    "@camunda/camunda-composite-components": "0.19.1",
    "@carbon/elements": "11.70.0",
    "@carbon/react": "1.57.0",
    "@devbookhq/splitter": "1.4.2",
    "@floating-ui/react-dom": "2.1.2",
    "@monaco-editor/react": "4.7.0",
    "@tanstack/eslint-plugin-query": "5.81.2",
    "@tanstack/react-query": "5.81.2",
    "@tanstack/react-query-devtools": "^5.67.1",
    "@vzeta/camunda-api-zod-schemas": "1.1.0",
    "bpmn-js": "18.6.2",
    "bpmn-moddle": "9.0.1",
    "date-fns": "4.1.0",
    "dmn-js": "17.1.0",
    "final-form": "4.20.10",
    "final-form-arrays": "3.1.0",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "mixpanel-browser": "2.66.0",
    "mobx": "6.13.5",
    "mobx-react": "9.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-router-dom": "7.6.3",
    "react-transition-group": "4.4.5",
    "serve": "14.2.4",
    "styled-components": "6.1.13"
  },
  "scripts": {
    "start": "vite",
    "build": "npm run ts-check && vite build && node license-build.js",
    "test": "react-scripts test --env=jsdom --resetMocks=false --clearMocks=true",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --resetMocks=false --ci --testTimeout 7000 --clearMocks=true",
    "ts-check": "tsc -b",
    "eslint": "eslint src/ e2e-playwright/ --max-warnings=0",
    "lint": "yarn ts-check && yarn eslint",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "test:e2e": "cross-env IS_E2E=true PORT=8081 ZEEBE_GATEWAY_ADDRESS=localhost:26503 ZEEBE_REST_GATEWAY_ADDRESS=http://localhost:8081 playwright test --project=e2e",
    "test:a11y": "playwright test --project=a11y",
    "test:visual": "playwright test --project=visual-light --project=visual-dark",
    "generate-screenshots": "playwright test --project=screenshot-generator",
    "test:e2e:ci": "cross-env IS_E2E=true PORT=8080 playwright test --project=e2e",
    "coverage": "yarn test --coverage --collectCoverageFrom=src/**/* --changedSince=origin/main --watchAll=false --passWithNoTests",
    "analyze-bundle": "npm run build && source-map-explorer 'build/assets/*.js'",
    "start-visual-regression-docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.53.2 /bin/bash",
    "start:visual-regression": "serve build/ -p 8081 -n -s -L",
    "build:visual-regression": "cross-env VITE_VERSION=0.0.0-SNAPSHOT npm run build",
    "extract-sbom": "vite build --mode sbom"
  },
  "devDependencies": {
    "@camunda8/sdk": "^8.6.8",
    "@eslint/js": "9.30.1",
    "@playwright/test": "1.53.2",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/carbon__elements": "11.10.3",
    "@types/carbon-components-react": "7.55.13",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.12",
    "@types/mixpanel-browser": "2.60.0",
    "@types/node": "22.16.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-transition-group": "4.4.11",
    "@vitejs/plugin-react": "4.6.0",
    "@vzeta/rollup-plugin-sbom": "2.0.0",
    "browserslist-to-esbuild": "2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "9.30.1",
    "eslint-plugin-license-header": "^0.8.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "event-source-polyfill": "1.0.31",
    "globals": "16.3.0",
    "jest-styled-components": "7.2.0",
    "monaco-editor": "0.52.2",
    "msw": "2.10.3",
    "prettier": "3.6.2",
    "react-test-renderer": "18.3.1",
    "rollup-plugin-license": "3.6.0",
    "sass": "1.89.2",
    "source-map-explorer": "2.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.35.1",
    "vite": "7.0.2",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "zod": "3.25.57"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scarfSettings": {
    "enabled": false
  },
  "msw": {
    "workerDirectory": "public"
  }
}
